{
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
  "regexManagers": [{
    "fileMatch": ["^Dockerfile$"],
    "matchStrings": ["ARG IREDADMIN_VERSION=(?<currentValue>.*?)\n"],
    "versioning": "loose",
    "depNameTemplate": "iredmail/iRedAdmin",
    "datasourceTemplate": "github-releases"
  }]
}
